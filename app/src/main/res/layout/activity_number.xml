<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:cl="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_number"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.popland.pop.a0_0.NumberActivity"
    android:background="#a276f2">
    <RelativeLayout
        android:id="@+id/RLlines"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <RelativeLayout
        android:id="@+id/RLcircle"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <ProgressBar
        android:id="@+id/PBimagination"
        android:layout_width="match_parent"
        android:layout_height="16dp"
        android:layout_alignParentTop="true"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:progress="0"
        android:max="21" />
    <io.github.francoiscampbell.circlelayout.CircleLayout
        android:id="@+id/CircleLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        cl:cl_angleOffset="-90"
        cl:cl_direction="clockwise"/>
    </RelativeLayout>
</FrameLayout>
